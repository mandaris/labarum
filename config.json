{
  "params": {
    "themeVersion": "1.3.18",
    "showAuthorInfo": true,
    "showDebugInfo": false,
    "themeDebug": true,
    "math": false
  },
  "markup": {
    "highlight": {
      "noClasses": false
    },
    "goldmark": {
      "parser": {
        "wrapStandAloneImageWithinParagraph": false,
        "attribute": {
          "block": true
        }
      },
      "extensions": {
        "passthrough": {
          "delimiters": {
            "block": [
              [
                "\\[",
                "\\]"
              ],
              [
                "$$",
                "$$"
              ]
            ],
            "inline": [
              [
                "\\(",
                "\\)"
              ]
            ]
          },
          "enable": true
        }
      }
    }
  }
}